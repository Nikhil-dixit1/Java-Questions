import java.util.*;
class Main{
    public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    
           int i ,j;
           int lines=sc.nextInt();
           for( i=1;i<=lines;i++){
               for(j=lines;j>0;j--){
                   if(j!=i)
                   {System.out.print(j);}
                   else System.out.print("*");
               }System.out.println();
           }
           
       }
    
}
